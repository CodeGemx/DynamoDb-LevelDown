{
  "name": "dynamodb-leveldown",
  "description": "A LevelDOWN API implementation of AWS DynamoDB.",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "test": "tape ./test/**.js | faucet",
    "dynalite": "dynalite --port 4567 --createTableMs 20 --deleteTableMs 20 --updateTableMs 20",
    "test-ci": "nyc tape ./test/**.js | tap-junit -s 'DynamoDBDown All Tests' -o build/junit -n results.xml || true"
  },
  "author": "Gio Palacino <gio@palacino.net>",
  "license": "Apache-2.0",
  "keywords": [
    "aws",
    "database",
    "db",
    "dynamo",
    "dynamodb",
    "storage",
    "store",
    "level",
    "leveldb",
    "levelup",
    "leveldown",
    "pouchdb"
  ],
  "dependencies": {
    "abstract-leveldown": "6.2.2",
    "through2": "3.0.1"
  },
  "devDependencies": {
    "aws-sdk": "2.585.0",
    "dynalite": "3.1.2",
    "faucet": "0.0.1",
    "levelup": "4.3.2",
    "nyc": "14.1.1",
    "semantic-release": "15.13.31",
    "standard": "14.3.1",
    "tap-junit": "3.1.0",
    "tap-nyc": "1.0.3",
    "tape": "4.11.0",
    "tape-catch": "1.0.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GioCirque/DynamoDb-LevelDown.git"
  },
  "release": {
    "branch": "master",
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "angular",
          "releaseRules": [
            {
              "type": "docs",
              "release": "patch"
            },
            {
              "type": "fix",
              "release": "patch"
            },
            {
              "type": "refactor",
              "release": "minor"
            },
            {
              "type": "style",
              "release": "patch"
            }
          ],
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES"
            ]
          }
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm"
    ]
  }
}
