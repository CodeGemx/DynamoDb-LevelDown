{
  "name": "DynamoDBDown",
  "description": "A LevelDOWN API implementation of AWS DynamoDB.",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "test": "tape ./test/**.js | faucet",
    "dynalite": "dynalite --port 4567 --createTableMs 20 --deleteTableMs 20 --updateTableMs 20",
    "test-ci": "mkdir -p build; tape ./test/**.js | tap-junit -s 'DynamoDBDown All Tests' -o build -n junit.xml",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "author": "Gio Palacino <gio@palacino.net>",
  "license": "Apache-2.0",
  "keywords": [
    "aws",
    "database",
    "db",
    "dynamo",
    "dynamodb",
    "storage",
    "store",
    "level",
    "leveldb",
    "levelup",
    "leveldown",
    "pouchdb"
  ],
  "dependencies": {
    "abstract-leveldown": "6.2.2",
    "through2": "3.0.1"
  },
  "devDependencies": {
    "aws-sdk": "2.585.0",
    "dynalite": "3.1.2",
    "faucet": "0.0.1",
    "levelup": "4.3.2",
    "semantic-release": "15.13.31",
    "standard": "14.3.1",
    "tap-junit": "3.1.0",
    "tape": "4.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GioCirque/DynamoDbDown.git"
  },
  "release": {
    "branch": "main"
  }
}
